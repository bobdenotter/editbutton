{
    "name": "bobdenotter/editbutton",
    "description": "A nifty little extension to add a button to quickly edit any record in Bolt.",
    "type": "bolt-extension",
    "keywords": [
        "edit",
        "button"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Bobdenotter\\EditButton\\": ""
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Bobdenotter\\EditButton\\EditButtonExtension"
    }
}
